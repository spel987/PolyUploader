      "endpoints": [
        "https://p-u.vercel.app/update-config"
      ],
      "windows": {
        "installMode": "quiet"
      }
    }
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
