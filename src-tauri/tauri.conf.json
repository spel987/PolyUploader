    "frontendDist": "../src"
  },
  "bundle": {
    "active": true,
    "targets": [
      "nsis"
    ],
    "windows": {
      "nsis": {
        "sidebarImage": "icons/sidebar.bmp",
        "installerIcon": "icons/setup_icon.ico",
        "installerHooks": "./windows/hooks.nsh",
        "installMode": "perMachine"
      }
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico",
      "icon.icns",
      "icon.png"
